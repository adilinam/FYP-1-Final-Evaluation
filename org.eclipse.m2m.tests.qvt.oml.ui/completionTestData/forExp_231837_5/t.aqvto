transformation zzz;

import Strings;
	
modeltype A uses ecore('http://www.eclipse.org/emf/2002/Ecore');

mapping main(in model: ecore::EPackage): ecore::EPackage {
	init {
		var abc : Sequence(String) := Sequence {'a', 'b', 'c', 'A', 'B', 'C'};
		var res : String := '';
		abc->forOne(i) {
			res := null;
		}; 
		/*@*/
		abc->forEach(i,j | j.equalsIgnoreCase('B')) {
			
			res := res + j;
		}; 
	}
}