<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.m2m.qvt.oml.runtime.qvtTransformation">
      <transformation
            file="Simpleuml_To_Rdb.qvto"
            id="Simpleuml_To_Rdb">
         <input
               metamodel="http://www.eclipse.org/qvt/1.0.0/Operational/examples/simpleuml"
               metaclass="simpleuml/Model">
         </input>
         <output
               metamodel="http://www.eclipse.org/qvt/1.0.0/Operational/examples/rdb"
               metaclass="rdb/Model">
         </output>
      </transformation>
   </extension>

</plugin>
