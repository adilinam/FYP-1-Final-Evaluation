modeltype ECORE uses 'http://www.eclipse.org/emf/2002/Ecore';
transformation bug425069a;

property eClassID = 'EClass';
property oclVoidID = 'OclVoid';
property oclAnyID = 'OclAny';

mapping EClass :: m() : EClass {
	name := eClassID;
}

mapping OclVoid :: m() : EClass {
	name := oclVoidID;	
}

mapping OclAny :: m() : EClass {
	name := oclAnyID;	
}
